<script type="text/javascript">

	  easy_button_on = new Image(); 
	  easy_button_on.src = '<%= image_path "find_jobs_on.gif" %>';
	
	  easy_button_off = new Image();
	  easy_button_off.src = '<%= image_path "find_jobs_off.gif" %>';	
	
	  

	function easyButton(state) {
		var button = document.getElementById("easy_button");
		if (state) {
			button.src = easy_button_on.src;
		} else {
			button.src = easy_button_off.src;
		}
	}
</script>



